  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "react-use-websocket": {
      "optional": true
    }
  },
  "devDependencies": {
    "@swc/core": "^1.13.2",
    "@tanstack/react-query": "^5.83.0",
